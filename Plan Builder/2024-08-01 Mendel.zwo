		<SteadyState Duration="75" Power="1.25"/>
		<SteadyState Duration="75" Power="0.4"/>
		<SteadyState Duration="60" Power="1.3"/>
		<SteadyState Duration="60" Power="0.4"/>
		<SteadyState Duration="45" Power="1.4"/>
		<SteadyState Duration="45" Power="0.4"/>
